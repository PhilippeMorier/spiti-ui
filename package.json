{
  "name": "spiti-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "start": "npm run start:development",
    "start:development": "ng serve --env=development",
    "build": "npm run build:development",
    "build:development": "ng build --env=development",
    "test": "ng test",
    "test:ci": "ng test --browsers HeadlessChrome Firefox",
    "lint": "ng lint --format codeFrame",
    "pree2e": "npm run tsc protractor.conf.ts",
    "postinstall": "check-node-version --package",
    "e2e": "ng e2e --env=development",
    "e2e:noserve": "npm run e2e -- --serve false",
    "coverage": "ng test --single-run true --code-coverage"
  },
  "private": true,
  "engines": {
    "npm": ">=5.3.0",
    "node": ">=8.3.0"
  },
  "dependencies": {
    "@angular/animations": "5.2.1",
    "@angular/cdk": "5.1.0",
    "@angular/common": "5.2.1",
    "@angular/compiler": "5.2.1",
    "@angular/core": "5.2.1",
    "@angular/forms": "5.2.1",
    "@angular/http": "5.2.1",
    "@angular/material": "5.1.0",
    "@angular/platform-browser": "5.2.1",
    "@angular/platform-browser-dynamic": "5.2.1",
    "@angular/router": "5.2.1",
    "angularfire2": "5.0.0-rc.4",
    "babylonjs": "3.2.0-alpha4",
    "class-validator": "0.7.3",
    "core-js": "2.5.3",
    "firebase": "4.9.0",
