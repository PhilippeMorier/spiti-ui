    "@angular/language-service": "5.2.1",
    "@types/jasmine": "2.8.4",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "9.3.0",
    "check-node-version": "3.2.0",
    "codelyzer": "4.1.0",
    "jasmine-core": "2.9.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.3.3",
    "karma-firefox-launcher": "1.1.0",
